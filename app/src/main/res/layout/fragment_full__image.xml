<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menu.gallarysub.Full_Image">

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"
        android:layout_marginTop="75dp"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullguideline1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15"/>



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullguideline2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7"/>
    <!-- TODO: Update blank fragment layout -->
    <ImageView
        android:id="@+id/iv_full"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/galic"
        app:layout_constraintBottom_toTopOf="@+id/fullgd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frameLayout3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullgd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="@id/fullgd"></FrameLayout>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullresult13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.83" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullresult14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullresult15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullresult16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.47" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullresult17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.53" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fullresult18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />
    <ImageView
        android:id="@+id/afterImg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/fullguideline1"
        app:layout_constraintLeft_toRightOf="@+id/fullresult15"
        app:layout_constraintRight_toLeftOf="@+id/fullresult16"
        app:layout_constraintTop_toTopOf="@+id/iv_full"
        android:src="@drawable/after1"

    />
    <ImageView
        android:id="@+id/beforeImg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/fullguideline1"
        app:layout_constraintLeft_toRightOf="@+id/fullresult17"
        app:layout_constraintRight_toLeftOf="@+id/fullresult18"
        app:layout_constraintTop_toTopOf="@+id/iv_full"
        android:src="@drawable/before2"

        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/const_gohome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/fullresult14"
        app:layout_constraintLeft_toRightOf="@+id/fullresult17"
        app:layout_constraintRight_toLeftOf="@+id/fullresult18"
        app:layout_constraintTop_toBottomOf="@+id/fullresult13"
        >

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.31" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.69" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.15" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.85" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.55" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/btnhome8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="87dp" />

        <ImageView
            android:id="@+id/img_gohome"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="centerInside"
            android:src="@drawable/save"
            app:layout_constraintBottom_toTopOf="@+id/btnhome6"
            app:layout_constraintLeft_toRightOf="@+id/btnhome1"
            app:layout_constraintRight_toLeftOf="@+id/btnhome2"
            app:layout_constraintTop_toBottomOf="@+id/btnhome5" />

        <TextView
            android:id="@+id/tv_gohome"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="저장하기"
            android:textColor="@color/blue"
            android:textSize="15dp"
            android:fontFamily="@font/pretendard_bold"
            app:layout_constraintBottom_toTopOf="@+id/btnhome8"
            app:layout_constraintLeft_toRightOf="@+id/btnhome3"
            app:layout_constraintRight_toLeftOf="@+id/btnhome4"
            app:layout_constraintTop_toBottomOf="@+id/btnhome7" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/const_share"
        android:layout_width="0dp"
        android:layout_height="0dp"

        app:layout_constraintBottom_toTopOf="@+id/fullresult14"
        app:layout_constraintLeft_toRightOf="@+id/fullresult15"
        app:layout_constraintRight_toLeftOf="@+id/fullresult16"
        app:layout_constraintTop_toBottomOf="@+id/fullresult13"
        >

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="27dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.69" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.15" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.85" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.55" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/share8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="87dp" />

        <ImageView
            android:id="@+id/img_share"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/share"
            app:layout_constraintBottom_toTopOf="@+id/share6"
            app:layout_constraintLeft_toRightOf="@+id/share1"
            app:layout_constraintRight_toLeftOf="@+id/share2"
            app:layout_constraintTop_toBottomOf="@+id/share5" />

        <TextView
            android:id="@+id/tv_share"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="공유하기"
            android:textColor="@color/blue"
            android:textSize="15dp"
            android:fontFamily="@font/pretendard_bold"
            app:layout_constraintBottom_toTopOf="@+id/share8"
            app:layout_constraintLeft_toRightOf="@+id/share3"
            app:layout_constraintRight_toLeftOf="@+id/share4"
            app:layout_constraintTop_toBottomOf="@+id/share7" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>